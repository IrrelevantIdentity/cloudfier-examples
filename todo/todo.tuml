package todo;

class Todo

    attribute description : String;
    
    attribute details : Memo;
    
    attribute status : Status := Open;

end;

enumeration Status
    Open;
    Done;
    Cancelled;
end;

class Comment
    attribute comment : Memo;
end;

composition
	navigable role comments : Comment[*];
    navigable role todo : Todo; 
end;

end.